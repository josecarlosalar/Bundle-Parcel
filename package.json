    },
    "homepage": "https://github.com/josecarlosalar/Lemoncode-Bundle-Parcel#readme",
    "devDependencies": {
        "parcel-bundler": "^1.12.5",
        "rimraf": "^4.1.1",
        "typescript": "^4.9.4"
    },
    "dependencies": {
        "@types/react": "^18.0.27",
        "@types/react-dom": "^18.0.10",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    }
